set dirs ""
set dots .
for {set i 1} {$i < $DEPTH} {incr i} {
	set dir  "{$i-1}"
	set dots $dots.
  		# Previous working directory
	set pwd [join $dirs "/"];
 	if [llength $dirs] {
		set pwd $pwd/
	}; # if
