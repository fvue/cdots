	set cwd  [join $dirs "/"]
	send "cd '$scriptDir/fixture1/$cwd'\r"
	expect -re $prompt/fixture1/$cwd>
		# Try completion
	set cmd "$dots \*2"
	set test "Should complete ${i}up with wildcard argument expanding to one possibility: $cmd"
	send "$cmd\t"
        # NOTE: "^$cmd\u0008\u00081-2/$" doesn't work, because the \* needs to be escaped again
        #       -- FVu, Fri Jun  8 02:04:06 CEST 2007
	set expected "^$dots \\\*2\b\b1-2/$"
	expect {
		-re $expected  { pass "`$test', output: '$expect_out(buffer)'" }
		-re $prompt.*> { fail "$test at prompt (expected '$expected', output: '$expect_out(buffer)'" }
		eof       { fail "`$test' at eof (expected '$expected', output: '$expect_out(buffer)'" }
		timeout   { fail "`$test' at timeout (expected '$expected', output: '$expect_out(buffer)'" }
	}; # expect
        # Try to really cd
	set test "Should cd ${i}up with wildcard argument expanding to one possibility: $cmd"
	send "\n"
	set expected $prompt/fixture1/1-2>
	expect {
		-re $expected    { pass "$test" }
		-re $prompt.*>   { fail "$test at prompt (expected '$expected', output: '$expect_out(buffer)'" }
		eof              { fail "$test at eof" }
		timeout          { fail "$test at timeout" }
	}; # expect
}; # for




